{
    "name": "wallet-service",
    "version": "0.0.1",
    "description": "Wallet Service for Digital Wallet Microservices",
    "author": {
        "name": "cornal",
        "email": "duy.nguyend.tpv@one-line.com"
    },
    "private": true,
    "license": "MIT",
    "scripts": {
        "prepare": "husky",
        "clean": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write '{src,test}/**/*.{ts,json,yml,yaml}' && prettier --write '*.{ts,json,yml,yaml,mjs}'",
        "start": "nest start",
        "start:dev": "nest start --watch src/main.ts",
        "start:prod": "node dist/src/main",
        "lint": "eslint",
        "lint:staged": "lint-staged",
        "lint:fix": "eslint --fix",
        "test": "TZ=UTC jest --config test/jest.json --passWithNoTests --forceExit",
        "deadcode": "ts-prune --project tsconfig.json --unusedInModule --skip *.json",
        "spell": "cspell lint --config cspell.json '{src,test}/**/*.ts' --color --gitignore --no-must-find-files --no-summary --no-progress || true",
        "package:upgrade": "npx npm-check-updates -u",
        "package:check": "npx npm-check-updates -i",
        "migrate:schema": "APP_ENV=migration nest start",
        "migrate:seed": "APP_ENV=migration nestjs-command seed:apikey",
        "migrate:remove": "APP_ENV=migration nestjs-command remove:apikey",
        "typeorm": "ts-node ./node_modules/typeorm/cli",
        "migration:run": "npm run typeorm migration:run -- -d src/common/postgres/typeorm.config.ts",
        "migration:generate": "npm run typeorm -- -d src/common/postgres/typeorm.config.ts migration:generate src/common/postgres/migrations/$npm_config_name",
        "migration:create": "npm run typeorm -- migration:create src/common/postgres/migrations/$npm_config_name",
        "migration:revert": "npm run typeorm -- -d src/common/postgres/typeorm.config.ts migration:revert",
        "migration:drop": "npm run typeorm -- -d src/common/postgres/typeorm.config.ts schema:drop"
    },
    "dependencies": {
        "@faker-js/faker": "^9.4.0",
        "@keyv/redis": "^4.2.0",
        "@nestjs/axios": "^4.0.0",
        "@nestjs/bullmq": "^11.0.1",
        "@nestjs/cache-manager": "^3.0.0",
        "@nestjs/common": "^11.0.7",
        "@nestjs/config": "^4.0.0",
        "@nestjs/core": "^11.0.7",
        "@nestjs/cqrs": "^11.0.3",
        "@nestjs/jwt": "^11.0.0",
        "@nestjs/mongoose": "^11.0.1",
        "@nestjs/passport": "^11.0.5",
        "@nestjs/platform-express": "^11.0.7",
        "@nestjs/swagger": "^11.0.3",
        "@nestjs/terminus": "^11.0.0",
        "@nestjs/throttler": "^6.4.0",
        "@nestjs/typeorm": "^11.0.0",
        "@sentry/nestjs": "^8.53.0",
        "@sentry/profiling-node": "^8.53.0",
        "axios": "^1.7.9",
        "bad-words": "^3.0.4",
        "bcryptjs": "^2.4.3",
        "body-parser": "^2.2.0",
        "bullmq": "^5.40.0",
        "cache-manager": "^6.4.0",
        "case": "^1.6.3",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "compression": "^1.7.5",
        "crypto-js": "^4.2.0",
        "eventemitter2": "^6.4.9",
        "helmet": "^8.0.0",
        "kafkajs": "^2.2.4",
        "luxon": "^3.5.0",
        "mongodb": "^6.18.0",
        "mongoose": "^8.9.6",
        "nestjs-command": "^3.1.4",
        "nestjs-i18n": "^10.5.0",
        "nestjs-pino": "^4.3.0",
        "oxide.ts": "^1.1.0",
        "passport": "^0.7.0",
        "passport-jwt": "^4.0.1",
        "pg": "^8.16.3",
        "pino-http": "^10.4.0",
        "pino-pretty": "^13.0.0",
        "reflect-metadata": "^0.2.2",
        "response-time": "^2.3.3",
        "rxjs": "^7.8.1",
        "typeorm": "^0.3.25",
        "xlsx": "^0.18.5",
        "yargs": "^17.7.2"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.7.1",
        "@commitlint/config-conventional": "^19.7.1",
        "@eslint/js": "^9.19.0",
        "@golevelup/ts-jest": "^0.6.2",
        "@nestjs/cli": "^11.0.2",
        "@nestjs/schematics": "^11.0.0",
        "@nestjs/testing": "^11.0.7",
        "@swc-node/register": "^1.10.9",
        "@swc/cli": "^0.6.0",
        "@swc/core": "^1.10.12",
        "@swc/jest": "^0.2.37",
        "@types/bad-words": "^3.0.3",
        "@types/bcryptjs": "^2.4.6",
        "@types/bytes": "^3.1.5",
        "@types/compression": "^1.7.5",
        "@types/cors": "^2.8.17",
        "@types/crypto-js": "^4.2.2",
        "@types/eslint__js": "^8.42.3",
        "@types/express": "^5.0.0",
        "@types/jest": "^29.5.14",
        "@types/lodash": "^4.17.15",
        "@types/luxon": "^3.4.2",
        "@types/ms": "^2.1.0",
        "@types/multer": "^1.4.12",
        "@types/node": "^22.13.0",
        "@types/passport-jwt": "^4.0.1",
        "@types/response-time": "^2.3.8",
        "@types/supertest": "^6.0.2",
        "@types/uuid": "^10.0.0",
        "cspell": "^8.17.3",
        "eslint": "^9.19.0",
        "eslint-config-prettier": "^10.0.1",
        "husky": "^9.1.7",
        "jest": "^29.7.0",
        "lint-staged": "^15.4.3",
        "prettier": "^3.4.2",
        "rxjs-marbles": "^7.0.1",
        "stop-only": "^3.4.2",
        "supertest": "^7.0.0",
        "ts-jest": "^29.2.5",
        "ts-loader": "^9.5.2",
        "ts-node": "^10.9.2",
        "ts-prune": "^0.10.3",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.7.3",
        "typescript-eslint": "^8.22.0"
    },
    "lint-staged": {
        "**/*.ts": [
            "prettier --write '{src,test}/**/*.{ts,json,yml,yaml}'",
            "eslint",
            "stop-only --file"
        ],
        "*.{json,yaml}": [
            "prettier --write"
        ]
    },
    "jest": {
        "transform": {
            "^.+\\.(t|j)s?$": [
                "@swc/jest"
            ]
        }
    }
}
